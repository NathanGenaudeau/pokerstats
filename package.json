{
  "name": "pokerstats",
  "version": "0.1",
  "description": "A poker statistics application",
  "author": "Nathan Genaudeau",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/NathanGenaudeau/pokerstats.git"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "kysely": "kysely",
    "kysely:migrate:latest": "kysely migrate:latest",
    "kysely:migrate:up": "kysely migrate:up",
    "kysely:migrate:down": "kysely migrate:down",
    "kysely:migrate:rollback:all": "kysely migrate:rollback --all",
    "kysely:prod": "kysely --config dist/kysely.config.js",
    "kysely:prod:migrate:latest": "pnpm run kysely:prod migrate:latest",
    "kysely:prod:migrate:up": "pnpm run kysely:prod migrate:up",
    "kysely:prod:migrate:down": "pnpm run kysely:prod migrate:down",
    "kysely:prod:migrate:rollback:all": "pnpm run kysely:prod migrate:rollback --all",
    "doc:generate": "npx compodoc -p tsconfig.json -n \"Documentation\"",
    "doc:serve": "npx compodoc -p tsconfig.json -n \"Documentation\" -s"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@nanogiants/nestjs-swagger-api-exception-decorator": "^1.7.2",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/devtools-integration": "^0.2.1",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@scalar/nestjs-api-reference": "^0.4.16",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dataloader": "^2.2.3",
    "envalid": "^8.1.0",
    "find-config": "^1.0.0",
    "graphql": "^16.11.0",
    "helmet": "^7.2.0",
    "kysely": "^0.27.6",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.16.3",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.10",
    "rxjs": "^7.8.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "kysely-ctl": "^0.11.1",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@9.11.0"
}
